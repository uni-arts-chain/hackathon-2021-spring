	"scripts": {
		"start-webapp": "yarn tsc && node build/src/index.js",
		"start-watcher": "yarn tsc && node --require dotenv/config build/src/polkadot/index.js",
		"start-task": "yarn tsc && node --require dotenv/config build/src/task/index.js",
